{% extends 'base.html.twig' %}

{% block title %}IssueField{% endblock %}

{% block body %}
    <h1>IssueField</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ issue_field.id }}</td>
            </tr>
            <tr>
                <th>_key</th>
                <td>{{ issue_field.key }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ issue_field.name }}</td>
            </tr>
            <tr>
                <th>Custom</th>
                <td>{{ issue_field.custom ? 'Yes' : 'No' }}</td>
            </tr>
            <tr>
                <th>Searchable</th>
                <td>{{ issue_field.searchable ? 'Yes' : 'No' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_issue_field_index') }}">back to list</a>

    <a href="{{ path('app_issue_field_edit', {'id': issue_field.id}) }}">edit</a>

    {{ include('issue_field/_delete_form.html.twig') }}
{% endblock %}
